params {
    appsConfig {
        applications {
            deeptmhmm = "medium"
            prints    = "heavy"
            tmbed     = "medium"
        }
        resources {
            light  = {
                memory = { 35.GB  * task.attempt }
                time   = { 12.h   * task.attempt }
            }
            medium = {
                memory = { 130.GB * task.attempt }
                time   = { 24.h   * task.attempt }
            }
            heavy  = {
                memory = { 250.GB * task.attempt }
                time   = { 18.h   * task.attempt }
            }
        }
    }
}
